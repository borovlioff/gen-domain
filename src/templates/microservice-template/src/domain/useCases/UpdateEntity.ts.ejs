import { <%= entity %> } from "../../domain/entities/<%= entity %>";
import { <%= entity %>Repository } from "../../domain/repositories/<%= entity %>Repository";

export class Update<%= entity %>{
  constructor(private repository: <%= entity %>Repository) {}

  async execute(
    id: string,
    updates: Partial<Omit<<%= entity %>, 'id' | 'createdAt'>>
  ): Promise<<%= entity %> | null> {
    return this.repository.updateOne({ id }, updates);
  }
}